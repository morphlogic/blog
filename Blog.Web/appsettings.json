{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=CSCWordPress;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PineBlogOptions": {
    "Title": "MorphBlog",
